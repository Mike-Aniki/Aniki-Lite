<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:i="clr-namespace:System.Windows.Interactivity;assembly=System.Windows.Interactivity"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib"
                    mc:Ignorable="d">


	<sys:Boolean x:Key="UseAbsoluteExtraMetadataPath">true</sys:Boolean>
	<sys:String x:Key="ExtraMetadataPath">..\..\..\..</sys:String>

	<sys:String x:Key="Gold">Gold</sys:String>
	<sys:String x:Key="ViewDetails">View Details</sys:String>
	<sys:String x:Key="HideDetails">Hide Details</sys:String>
	<sys:String x:Key="HideTrophy">Hide Trophies</sys:String>
	<sys:String x:Key="Trophy">View Trophies</sys:String>
	<sys:String x:Key="UserName">User Name</sys:String>
	<sys:Boolean x:Key="FilterVariante">True</sys:Boolean>
	<sys:Boolean x:Key="TrailerPlayAuto">False</sys:Boolean>
	<sys:Boolean x:Key="DetailsAuto">True</sys:Boolean>
	<sys:Int32 x:Key="AvatarImageIndex">1</sys:Int32>
	<sys:Boolean x:Key="AnimationBackground">False</sys:Boolean>

	<Color x:Key="BlackColor">#FF000000</Color>
	<Color x:Key="WhiteColor">#FFFFFFFF</Color>
	<Color x:Key="NoPlugin">#eaeaea</Color>
	<Color x:Key="GlyphColor">#FF101010</Color>
	<Color x:Key="GlyphLightColor">#FF2A2A2A</Color>
	<Color x:Key="SelectionLightColor">#FF3A3A3A</Color>
	<Color x:Key="OverlayColor">#BB000000</Color>
	<Color x:Key="ControlBackgroundColor">#FF1A1A1A</Color>
	<Color x:Key="ControlBackgroundColorDark">#FF0F0F0F</Color>
	<Color x:Key="ColorBorder">#3B9EFF</Color>
	<Color x:Key="TextColor">#FFE0E0E0</Color>
	<Color x:Key="Text2Color">#FF757575</Color>
	<Color x:Key="TextFocusColor">#2B6FDB</Color>
	<Color x:Key="TextColorDark">#FF4A4A4A</Color>
	<Color x:Key="ButtonColorBackground">#FF101010</Color>
	<Color x:Key="TrophyBackground">#FF1E1E1E</Color>
	<Color x:Key="TrophyBackgroundFocus">#FF2E2E2E</Color>

	<Color x:Key="BorderGame">#444444</Color>
	<Color x:Key="ButtonFocusColorBackground">#FF2A2A2A</Color>
	<Color x:Key="TrophyBackgroundNoFocus">#FF111111</Color>
	<Color x:Key="DetailsBackgroundTop">#AA1A1A1A</Color>
	<Color x:Key="DetailsBackgroundBottom">#FF101010</Color>
	<Color x:Key="DetailsBackgroundTop2">#AA2A2E34</Color>
	<Color x:Key="DetailsBackgroundBottom2">#FF101010</Color>
	<Color x:Key="ProgressBarBackground">#FF2A2A2A</Color>
	<Color x:Key="ProgressBarForeground">#FF3ABEFF</Color>

	<Color x:Key="BorderWindow">#FF444444</Color>
	<Color x:Key="BackgroundColorWindowOpen">#BB000000</Color>




	<SolidColorBrush x:Key="NoPluginBrush" Color="{DynamicResource NoPlugin}" />
	<SolidColorBrush x:Key="ButtonBrushBackground" Color="{DynamicResource ButtonColorBackground}" />
	<SolidColorBrush x:Key="TextBrush" Color="{DynamicResource TextColor}" />
	<SolidColorBrush x:Key="Text2Brush" Color="{DynamicResource Text2Color}" />
	<SolidColorBrush x:Key="TextBrushDark" Color="{DynamicResource TextColorDark}" />
	<SolidColorBrush x:Key="GlyphBrush" Color="{DynamicResource GlyphColor}" />
	<SolidColorBrush x:Key="GlyphLightBrush" Color="{DynamicResource GlyphLightColor}" />
	<SolidColorBrush x:Key="ControlBackgroundBrush" Color="{DynamicResource ControlBackgroundColor}" />
	<SolidColorBrush x:Key="ControlBackgroundDarkBrush" Color="{DynamicResource ControlBackgroundColorDark}" />
	<SolidColorBrush x:Key="SelectionLightBrush" Color="{DynamicResource SelectionLightColor}" />
	<SolidColorBrush x:Key="OverlayBrush" Color="{DynamicResource OverlayColor}" />
	<SolidColorBrush x:Key="CheckBoxCheckMarkBkBrush" Color="{DynamicResource GlyphLightColor}" />
	<SolidColorBrush x:Key="UninstalledColor" Color="{DynamicResource WhiteColor}" />
	<SolidColorBrush x:Key="ColorBorderBrush" Color="{DynamicResource ColorBorder}" />
	<SolidColorBrush x:Key="TextFocusColorBrush" Color="{DynamicResource TextFocusColor}" />
	<SolidColorBrush x:Key="TrophyBackgroundBrush" Color="{DynamicResource TrophyBackground}" />
	<SolidColorBrush x:Key="TrophyBackgroundFocusBrush" Color="{DynamicResource TrophyBackgroundFocus}" />

	<SolidColorBrush x:Key="BorderGameBrush" Color="{DynamicResource BorderGame}" />
	<SolidColorBrush x:Key="ButtonFocusBrushBackground" Color="{DynamicResource ButtonFocusColorBackground}" />
	<SolidColorBrush x:Key="TrophyBackgroundNoFocusBrush" Color="{DynamicResource TrophyBackgroundNoFocus}" />
	<SolidColorBrush x:Key="DetailsBackgroundTopBrush" Color="{DynamicResource DetailsBackgroundTop}" />
	<SolidColorBrush x:Key="DetailsBackgroundBottomBrush" Color="{DynamicResource DetailsBackgroundBottom}" />

	<SolidColorBrush x:Key="DetailsBackgroundTopBrush2" Color="{DynamicResource DetailsBackgroundTop2}" />
	<SolidColorBrush x:Key="DetailsBackgroundBottomBrush2" Color="{DynamicResource DetailsBackgroundBottom2}" />

	<SolidColorBrush x:Key="ProgressBarBackgroundBrush" Color="{DynamicResource ProgressBarBackground}" />
	<SolidColorBrush x:Key="ProgressBarForegroundBrush" Color="{DynamicResource ProgressBarForeground}" />
	<SolidColorBrush x:Key="BorderWindowBrush" Color="{DynamicResource BorderWindow}" />

	<SolidColorBrush x:Key="MainBackgourndBrush" Color="Black" />
	<SolidColorBrush x:Key="WarningBrush" Color="#ff6b6b" />

	<sys:Double x:Key="FontSizeSmall">18</sys:Double>
	<sys:Double x:Key="FontSize">25</sys:Double>

	<sys:Double x:Key="FilterPanelWidth">300</sys:Double>

    <LinearGradientBrush x:Key="OverlayMenu" StartPoint="0,1" EndPoint="1,0" >
        <GradientStop Color="#FF000000" Offset="0.1"/>
        <GradientStop Color="#FF000000" Offset="4"/>
    </LinearGradientBrush>

	<LinearGradientBrush x:Key="SuccessBackground" StartPoint="0,1" EndPoint="1,0" >
		<GradientStop Color="Black" Offset="0"/>
		<GradientStop Color="Black" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="StaticFlashMenu">
		<GradientStop Color="#00000000 " Offset="0"/>
		<GradientStop Color="#40ffffff" Offset="1"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="UninstalledColorBrush" StartPoint="-0.5,0.5" EndPoint="1,0.5" >
		<GradientStop Color="DarkSlateGray" Offset="0"/>
		<GradientStop Color="LightSlateGray" Offset="1.0"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="TopBarBackgroundBrush" StartPoint="-0.5,0.5" EndPoint="1,0.5" >
		<GradientStop Color="#161616" Offset="0"/>
		<GradientStop Color="#161616" Offset="1.0"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="TrailerBackgroundBrush" StartPoint="-0.5,0.5" EndPoint="1.5,1" >
		<GradientStop Color="#2c2c2c" Offset="0"/>
		<GradientStop Color="Black" Offset="1.0"/>
	</LinearGradientBrush>

	<LinearGradientBrush x:Key="GameDetailsBrush" StartPoint="0,0" EndPoint="1,0">
		<GradientStop Color="#FF000000" Offset="0.0"/>
		<GradientStop Color="#00000000" Offset="0.7"/>
	</LinearGradientBrush>

	<Style x:Key="TextBlockBaseStyle" TargetType="TextBlock">
		<Setter Property="FontSize" Value="{DynamicResource FontSize}" />
		<Setter Property="FontFamily" Value="Segoe Ui SemiBold" />
		<Setter Property="Foreground" Value="{DynamicResource TextBrush}" />
	</Style>

	<Style x:Key="TextBlockDetailsStyleTop" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe UI Bold" />
		<Setter Property="Foreground" Value="{DynamicResource TextBrush}" />
		<Setter Property="Typography.Capitals" Value="AllSmallCaps"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSize}" />
	</Style>

	<Style x:Key="TextBlockDetailsStyleBottom" TargetType="TextBlock">
		<Setter Property="FontFamily" Value="Segoe UI" />
		<Setter Property="Foreground" Value="{DynamicResource TextBrush}" />
		<Setter Property="FontSize" Value="{DynamicResource FontSize}" />
	</Style>



	<Style x:Key="TextBlockBaseStyleForDateUnlock" TargetType="TextBlock">
		<Setter Property="FontSize" Value="{DynamicResource FontSize}" />
		<Setter Property="FontFamily" Value="Segoe Ui SemiBold" />
		<Setter Property="Foreground" Value="{DynamicResource TextBrush}" />
		<Style.Triggers>
			<DataTrigger Binding="{Binding ElementName=SuccessChangeToggle, Path=IsChecked}" Value="True">
				<Setter Property="Visibility" Value="Collapsed"/>
			</DataTrigger>
		</Style.Triggers>
	</Style>


    <ImageBrush x:Key="SettingBackground" ImageSource="{ThemeFile 'Images/SettingDark.png'}" Stretch="UniformToFill" AlignmentX="Right" AlignmentY="Bottom"/>
	<ImageBrush x:Key="SettingBackground219" ImageSource="{ThemeFile 'Themes Option/4.Ratio/21.9/SettingDark219.png'}" Stretch="UniformToFill" AlignmentX="Right" AlignmentY="Bottom"/>
	<ImageBrush x:Key="SettingBackground1610" ImageSource="{ThemeFile 'Themes Option/4.Ratio/16.10/SettingDark1610.png'}" Stretch="UniformToFill" AlignmentX="Right" AlignmentY="Bottom"/>

    <BitmapImage x:Key="ThemeLogo" UriSource="{ThemeFile 'Images/For Dark Version/ThemeLogo.png'}" />
    <BitmapImage x:Key="BCritical" UriSource="{ThemeFile 'Images/For Dark Version/Battery_Critical.png'}" />
    <BitmapImage x:Key="BLow" UriSource="{ThemeFile 'Images/For Dark Version/Battery_Low.png'}" />
